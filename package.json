    "passport-http-bearer": "1.0.1",
    "yaml": "2.5.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "16.18.105",
    "@types/passport": "1.0.16",
    "@types/passport-anonymous": "1.0.5",
    "@types/passport-http-bearer": "1.0.41",
    "@types/yargs": "17.0.33",
    "tslint": "6.1.3",
    "typescript": "5.5.4"
  },
  "bin": {
    "juice-ws": "./juice-ws.js"
  }
