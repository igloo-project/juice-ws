{
  "name": "juice-ws",
  "version": "0.5.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf dist/ && rm -rf juice-ws*.tgz",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node . server -p 8000",
